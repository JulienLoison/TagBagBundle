{
    "name": "setono/tag-bag-bundle",
    "description": "A Symfony bundle that creates a session bag called tag which can be used to inject i.e. script tags on your website",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Joachim LÃ¸vgaard",
            "email": "joachim@loevgaard.dk"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/config": "^4.1",
        "symfony/dependency-injection": "^4.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/http-foundation": "^4.1"
    },
    "require-dev": {
        "phpspec/phpspec": "^5.1",
        "phpstan/phpstan": "^0.10.7",
        "roave/security-advisories": "dev-master",
        "symfony/templating": "^4.1",
        "symfony/twig-bundle": "^4.1",
        "symplify/easy-coding-standard": "^5.3"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Setono\\TagBagBundle\\": ""
        },
        "exclude-from-classmap": [ "/Tests/" ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "analyse": "./vendor/bin/phpstan analyse -c phpstan.neon -l max",
        "check-style": "./vendor/bin/ecs check DependencyInjection/ HttpFoundation/ Tag/ Twig/",
        "fix-style": "./vendor/bin/ecs check --fix DependencyInjection/ HttpFoundation/ Tag/ Twig/",
        "test": "./vendor/bin/phpspec run"
    }
}
